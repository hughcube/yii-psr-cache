{
  "name": "hughcube/yii-psr-cache",
  "description": "The cache extension for the Yii framework",
  "license": "MIT",
  "authors": [
    {
      "name": "hugh.li",
      "email": "hugh.li@foxmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "*",
    "psr/simple-cache": "*"
  },
  "autoload": {
    "psr-4": {
      "Hughcube\\YiiPsrCache\\": "src"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1",
    "mockery/mockery": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Hughcube\\YiiPsrCache\\Tests\\": "tests"
    }
  }
}
